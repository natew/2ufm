<%# cache ['v1', 'genres', current_user.updated_at] do %>
  <%-
    selected = Genre.active.ordered.users(current_user)
    not_selected = Genre.active.ordered.not_users(current_user)
  -%>
  <% if selected.length > 0 %>
    <%= render partial: 'genres/link', collection: selected %>
  <% else %>
    <%= render partial: 'genres/link', collection: not_selected %>
  <% end %>
<%# end %>