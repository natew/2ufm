<%= render :partial => 'shared/stations_nav' %>

<div class="whole">
  <p>Browse blogs and subscribe to their stations. <%= link_to 'Submit your blog', new_blog_path %></p>

  <%= render partial: 'shared/stations_subnav', locals: { object: 'blogs' } %>

  <div class="stations">
    <%= render :partial => 'stations/station', :collection => @blogs %>
    <%= paginate @blogs if @blogs.respond_to? 'current_page' %>
  </div>
</div>